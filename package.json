{
  "name": "i18n",
  "description": "lightweight translation module with dynamic json storage",
  "version": "0.9.1",
  "homepage": "http://github.com/mashpie/i18n-node",
  "repository": {
    "type": "git",
    "url": "http://github.com/mashpie/i18n-node.git"
